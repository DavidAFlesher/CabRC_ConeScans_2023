#!/bin/csh
# matrix transposition
{if(NR==1) {print$0};
 if(NR==2) {r[1,1]=$1; r[1,2]=$2; r[1,3]=$3};
 if(NR==3) {r[2,1]=$1; r[2,2]=$2; r[2,3]=$3};
 if(NR==4) {r[3,1]=$1; r[3,2]=$2; r[3,3]=$3};
 if(NR==5) {t[1]=$1; t[2]=$2; t[3]=$3}}
END{
printf("%17.7f%17.7f%17.7f\n",r[1,1],r[2,1],r[3,1]);
printf("%17.7f%17.7f%17.7f\n",r[1,2],r[2,2],r[3,2]);
printf("%17.7f%17.7f%17.7f\n",r[1,3],r[2,3],r[3,3]);
printf("%17.7f%17.7f%17.7f\n",t[1],t[2],t[3])}
